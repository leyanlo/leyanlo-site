{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^2.0.0-rc.3",
    "gatsby-cli": "^2.0.0-rc.0",
    "gatsby-image": "^2.0.0-rc.0",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-manifest": "^2.0.2-rc.0",
    "gatsby-plugin-offline": "^2.0.0-rc.0",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.0",
    "gatsby-plugin-sharp": "^2.0.0-rc.3",
    "gatsby-source-filesystem": "^2.0.1-rc.0",
    "gatsby-transformer-sharp": "^2.1.1-rc.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "npm-run-all -p format:js format:css",
    "format:js": "prettier --write 'src/**/*.{js,jsx}'",
    "format:css": "prettier --write 'src/**/*.css'",
    "lint": "npm-run-all -p lint:es lint:style",
    "lint:es": "eslint --fix --ignore-path .gitignore 'src/**/*.{js,jsx,md,yml}'",
    "lint:style": "stylelint --fix 'src/**/*.css'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-rc.2",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-plugin-react-svg": "^2.0.0-beta1",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-rational-order": "0.0.2",
    "stylelint-config-standard": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default",
    "//": "TODO: finalize repo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md,yml}": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  }
}
